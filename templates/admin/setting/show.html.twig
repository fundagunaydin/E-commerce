{% extends 'adminbase.html.twig' %}

{% block title %}Setting{% endblock %}

{% block body %}

    <div id="content">
        <!--breadcrumbs-->
        <div id="content-header">
            <div id="breadcrumb"><a href="/admin" title="Go to Home" class="tip-bottom"><i class="icon-home"></i>
                    Home</a> <a href="{{ path('setting_index') }}">Setting</a> <a href="#" class="current">Show</a></div>
        </div>
        <!--End-breadcrumbs-->

        <div class="row-fluid">

            <div class="widget-title bg_lg"><span class="icon"><i class="icon-align-justify"></i></span>
                <h5>Setting List Table</h5>

            </div>

        </div>

        <div class="widget-content nopadding">
            <table class="table table-bordered table-striped">
                <tbody>
                <tr>
                    <th>Id</th>
                    <td>{{ setting.id }}</td>
                </tr>

                <tr>
                    <th>Title</th>
                    <td>{{ setting.title }}</td>
                </tr>
                <tr>
                    <th>Keywords</th>
                    <td>{{ setting.keywords }}</td>
                </tr>
                <tr>
                    <th>description</th>
                    <td>{{ setting.description }}</td>
                </tr>

                <tr>
                    <th>Company</th>
                    <td>{{ setting.company }}</td>
                </tr>
                <tr>
                    <th>Address</th>
                    <td>{{ setting.address }}</td>
                </tr>
                <tr>
                    <th>Phone</th>
                    <td>{{ setting.phone }}</td>
                </tr>
                <tr>
                    <th>Email</th>
                    <td>{{ setting.email }}</td>
                </tr>
                <tr>
                    <th>SmtpServer</th>
                    <td>{{ setting.smtpServer | raw }}</td>
                </tr>
                <tr>
                    <th>SmtpEmail</th>
                    <td>{{ setting.Smtpemail | raw }}</td>
                </tr>
                <tr>
                    <th>SmtpPassword</th>
                    <td>{{ setting.Smtppassword | raw }}</td>
                </tr>
                <tr>
                    <th>SmptPort</th>
                    <td>{{ setting.Smtpport | raw }}</td>
                </tr>

                <tr>
                    <th>Facebook</th>
                    <td>{{ setting.facebook }}</td>
                </tr>
                <tr>
                    <th>Instagram</th>
                    <td>{{ setting.instagram }}</td>
                </tr>
                <tr>
                    <th>Twitter</th>
                    <td>{{ setting.twitter }}</td>
                </tr>
                <tr>
                    <th>Aboutus</th>
                    <td>{{ setting.aboutus | raw }}</td>
                </tr>
                <tr>
                    <th>Contact</th>
                    <td>{{ setting.contact | raw }}</td>
                </tr>
                <tr>
                    <th>Reference</th>
                    <td>{{ setting.reference | raw }}</td>
                </tr>


                <tr>
                    <th>Status</th>
                    <td>{{ setting.status }}</td>
                </tr>
                </tbody>
            </table>
        </div>

    </div>




{% endblock %}

